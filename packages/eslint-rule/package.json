{
  "name": "@korix/eslint-rule",
  "version": "0.0.1-20250707",
  "description": "ESLint rules for Kori framework",
  "type": "module",
  "author": "mitz (@bufferings)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bufferings/kori"
  },
  "homepage": "https://github.com/bufferings/kori",
  "bugs": "https://github.com/bufferings/kori/issues",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "clean": "rimraf dist *.tsbuildinfo .turbo .tsup",
    "build": "tsdown",
    "test": "vitest"
  },
  "dependencies": {
    "@typescript-eslint/types": "catalog:",
    "@typescript-eslint/utils": "catalog:"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "catalog:",
    "@typescript-eslint/rule-tester": "catalog:",
    "@types/eslint": "catalog:"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  }
}
