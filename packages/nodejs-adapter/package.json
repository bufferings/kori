{
  "name": "@korix/nodejs-adapter",
  "version": "0.0.1-20250708.8",
  "description": "Node.js adapter for Kori framework",
  "type": "module",
  "author": "mitz (@bufferings)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bufferings/kori"
  },
  "homepage": "https://github.com/bufferings/kori",
  "bugs": "https://github.com/bufferings/kori/issues",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "clean": "rimraf dist *.tsbuildinfo .turbo .tsup",
    "build": "tsdown"
  },
  "dependencies": {
    "@hono/node-server": "1.15.0",
    "@korix/kori": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsdown": "^0.12.9",
    "typescript": "catalog:"
  }
}
