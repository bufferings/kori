{
  "name": "@korix/zod-schema",
  "version": "0.0.1-20250707",
  "description": "Zod schema integration for Kori framework",
  "type": "module",
  "author": "mitz (@bufferings)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bufferings/kori"
  },
  "homepage": "https://github.com/bufferings/kori",
  "bugs": "https://github.com/bufferings/kori/issues",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "clean": "rimraf dist *.tsbuildinfo .turbo",
    "build": "tsup && tsc -p tsconfig.publish.json"
  },
  "dependencies": {
    "@korix/kori": "workspace:*"
  },
  "peerDependencies": {
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "zod": "3.25.67"
  }
}
