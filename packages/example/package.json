{
  "name": "@korix/example",
  "version": "0.1.0",
  "private": true,
  "description": "Usage examples for Kori framework",
  "type": "module",
  "author": "mitz (@bufferings)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bufferings/kori"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo .turbo .tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev:getting-started": "cross-env NODE_OPTIONS='--conditions=development' tsx watch src/getting-started.ts",
    "dev:advanced-usage": "cross-env NODE_OPTIONS='--conditions=development' tsx watch src/advanced-usage.ts",
    "test": "echo 'No unit tests' && exit 0",
    "test:watch": "echo 'No unit tests' && exit 0"
  },
  "dependencies": {
    "@korix/kori": "workspace:*",
    "@korix/nodejs-server": "workspace:*",
    "@korix/openapi-plugin": "workspace:*",
    "@korix/openapi-swagger-ui-plugin": "workspace:*",
    "@korix/standard-schema-adapter": "workspace:*",
    "@korix/zod-openapi-plugin": "workspace:*",
    "@korix/zod-schema-adapter": "workspace:*",
    "openapi3-ts": "4.4.0",
    "zod": "4.0.0"
  },
  "devDependencies": {
    "@korix/eslint-config": "workspace:*",
    "@types/node": "catalog:",
    "cross-env": "7.0.3",
    "eslint": "catalog:",
    "pino-pretty": "13.0.0",
    "rimraf": "catalog:",
    "tsx": "4.19.2",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
