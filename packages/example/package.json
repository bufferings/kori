{
  "name": "kori-example",
  "version": "0.0.1",
  "private": true,
  "description": "Usage examples for Kori framework",
  "type": "module",
  "author": "mitz (@bufferings)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bufferings/kori"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev:clean": "rimraf dist-dev tsconfig.tsbuildinfo",
    "dev:build": "tsc",
    "dev:watch": "tsc -w",
    "dev:getting-started": "NODE_ENV=development tsx --watch src/getting-started.ts",
    "dev:usage": "NODE_ENV=development tsx --watch src/usage.ts"
  },
  "dependencies": {
    "@korits/kori": "workspace:*",
    "kori-body-limit-plugin": "workspace:*",
    "kori-cors-plugin": "workspace:*",
    "kori-nodejs-adapter": "workspace:*",
    "kori-openapi-plugin": "workspace:*",
    "kori-openapi-scalar-ui-plugin": "workspace:*",
    "kori-pino-adapter": "workspace:*",
    "kori-zod-openapi-plugin": "workspace:*",
    "kori-zod-schema": "workspace:*",
    "kori-zod-validator": "workspace:*",
    "pino": "9.7.0",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "pino-pretty": "13.0.0",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
