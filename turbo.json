{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "dev:build": {
      "dependsOn": ["^dev:build"],
      "outputs": ["dist-dev/**"]
    },
    "kori-example#dev:build": {
      "dependsOn": ["^dev:build"],
      "outputs": []
    },
    "dev:watch": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^dev:build"]
    },
    "dev:getting-started": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^dev:build"]
    },
    "dev:usage": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^dev:build"]
    },
    "test": {
      "cache": false,
      "dependsOn": ["^publish:build"]
    },
    "publish:build": {
      "dependsOn": ["^publish:build"],
      "outputs": ["dist/**"]
    }
  }
}
