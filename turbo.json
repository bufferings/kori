{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "typecheck": {},
    "lint": {},
    "lint:fix": {},
    "clean": {},
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "@korix/example#dev:getting-started": {
      "cache": false,
      "persistent": true,
